import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc= new Scanner(System.in);
		
		int n;
		System.out.println("Input n" );
		n = sc.nextInt();
		
		
		for (int i = 1; i <=6; i++) {
			for (int j=1; j<=n/2; j++) {
				System.out.print("***");
				System.out.print("+++");
				}
			if (n%2!=0) {
				System.out.print("***");}
			
			
			System.out.println();
				

		}
	}
}
