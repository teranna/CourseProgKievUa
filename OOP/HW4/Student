
public class Student extends People{
	
	private String group;

	
	public Student (String name, String lastname, int age, String sex, String group) {
		super(name, lastname,age, sex );
		
		this.group=group;
		
		}
	
	public Student () {
		super ();
	}

	public String getGroup() {
		return group;
	}

	public void setGroup(String group) {
		this.group = group;
	}

	

	@Override
	public String toString() {
		return "Student [group=" + group + "]";
	}
	

}
