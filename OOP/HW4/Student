
public class People {

	private String name;
	private String lastname;
	private int age;
	private Sex sex;

	public People(String name, String lastname, int age, String sex) {
		super();
		this.lastname = lastname;
		this.name = name;
		this.age = age;

	}

	public People () {
		
	}
		
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getLastname() {
		return lastname;
	}

	public void setLastname(String lastname) {
		this.lastname = lastname;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public Sex getSex() {
		return sex;
	}

	public void setSex(Sex sex) {
		this.sex = sex;
	}

	@Override
	public String toString() {
		return "People [name=" + name + ", lastname=" + lastname + ", age=" + age + ", sex=" + sex + "]";
	}

	
	
	
}
