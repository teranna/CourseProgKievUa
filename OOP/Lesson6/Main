
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Thread maiThread=Thread.currentThread();
		
		System.out.println(maiThread.getName()+ " START");
		
		try {
			Thread.sleep(2000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		
		
		System.out.println(maiThread.getName()+" STOP");
		System.out.println(maiThread.getId());
	}

}
